local function showLoadingAnimation()
    local loadingScreen = Instance.new("ScreenGui")
    loadingScreen.Parent = game.CoreGui
    
    local imageLabel = Instance.new("ImageLabel")
    imageLabel.Size = UDim2.new(0, 300, 0, 300)
    imageLabel.Position = UDim2.new(0.5, -150, 0.5, -150)
    imageLabel.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
    imageLabel.BackgroundTransparency = 0
    imageLabel.Image = "rbxassetid://84970149824503"
    imageLabel.ImageTransparency = 1
    imageLabel.Parent = loadingScreen
    
    local mainText = Instance.new("TextLabel")
    mainText.Size = UDim2.new(1, 0, 0.7, 0)
    mainText.Position = UDim2.new(0, 0, 0.1, 0)
    mainText.BackgroundTransparency = 1
    mainText.Text = "ðŸ”®\nNAMELESS\nV1.1"
    mainText.TextColor3 = Color3.new(1, 1, 1)
    mainText.TextSize = 24
    mainText.Font = Enum.Font.GothamBold
    mainText.TextTransparency = 1
    mainText.TextWrapped = true
    mainText.Parent = imageLabel
    
    local splashText = Instance.new("TextLabel")
    splashText.Size = UDim2.new(1, 0, 0, 20)
    splashText.Position = UDim2.new(0, 0, 0.8, 0)
    splashText.BackgroundTransparency = 1
    splashText.Text = "by Splash"
    splashText.TextColor3 = Color3.new(1, 1, 1)
    splashText.TextTransparency = 0.7
    splashText.TextSize = 16
    splashText.Font = Enum.Font.Gotham
    splashText.TextTransparency = 1
    splashText.Parent = imageLabel
    
    for i = 1, 10 do
        imageLabel.ImageTransparency = imageLabel.ImageTransparency - 0.1
        mainText.TextTransparency = mainText.TextTransparency - 0.1
        splashText.TextTransparency = splashText.TextTransparency - 0.1
        wait(0.1)
    end
    
    wait(3)
    
    for i = 1, 10 do
        imageLabel.ImageTransparency = imageLabel.ImageTransparency + 0.1
        mainText.TextTransparency = mainText.TextTransparency + 0.1
        splashText.TextTransparency = splashText.TextTransparency + 0.1
        wait(0.1)
    end
    
    loadingScreen:Destroy()
end

local function loadScriptByPlaceId()
    local placeId = game.PlaceId
    local scripts = {
        [6961824067] = function()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/millioned/Nameless/refs/heads/main/Namelesss'))()
        end,
    }

    if scripts[placeId] then
        scripts[placeId]()
    end
end
showLoadingAnimation()
loadScriptByPlaceId()
